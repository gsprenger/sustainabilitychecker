<div class='cont'>
  <!-- navarrows -->
  <a href='<%= demand_path(anchor: 'households') %>'>
    <div class='navarrow navarrow-left'>
      <i class="fa fa-chevron-left fa-4x"></i>
    </div>    
  </a>
  <a href='<%= check_path %>'>
    <div class='navarrow navarrow-right'>
      <i class="fa fa-chevron-right fa-4x"></i>
    </div>    
  </a>

<!-- accordion -->
  <div class="panel-group" id="accordion">
    <!-- agriculture -->
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a class="panel-toggle" data-toggle="collapse" data-parent="#accordion" href="#agriculture">
            Agriculture
          </a>
        </h4>
      </div>
      <div id="agriculture" class="panel-collapse collapse<%= " in" if @section == 'agriculture' %>">
        <div class="panel-body">
        </div>
      </div>
    </div>
    <!-- energy -->
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a class="panel-toggle" data-toggle="collapse" data-parent="#accordion" href="#energy">
            Energy
          </a>
        </h4>
      </div>
      <div id="energy" class="panel-collapse collapse<%= " in" if @section == 'energy' %>">
        <div class="panel-body">
        </div>
      </div>
    </div>
    <!-- industrialization -->
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a class="panel-toggle" data-toggle="collapse" data-parent="#accordion" href="#industrialization">
            Industrialization
          </a>
        </h4>
      </div>
      <div id="industrialization" class="panel-collapse collapse<%= " in" if @section == 'industrialization' %>">
        <div class="panel-body">
        </div>
      </div>
    </div>
  </div>
</div>